    "panelTitle.activeBorder": "--transparent:transparent_light--", // Border color for the active panel title.
    "panelTitle.activeForeground": "--white:white_light--", // Title color for the active panel.
    "panelTitle.inactiveForeground": "--bluewhite40:bluewhite40_light--", // Title color for the inactive panel.
    "panelInput.border": "--background1_border:background1_border_light--", // Input box border for inputs in the panel.
    "imagePreview.border": "--background2_border:background2_border_light--", // Border color for image in image preview.
    // Status bar colors
    "statusBar.background": "--background1:background1_light--", // Standard Status Bar background color.
    "statusBar.foreground": "--white:white_light--", // Status Bar foreground color.
    "statusBar.border": "--transparent:transparent_light--", // Status Bar border color separating the Status Bar and editor.
    "statusBar.debuggingBackground": "--blue:hi_blue_light--", // Status Bar background color when a program is being debugged.
    "statusBar.debuggingForeground": "--white:white_light--", // Status Bar foreground color when a program is being debugged.
    "statusBar.debuggingBorder": "--transparent:transparent_light--", // Status Bar border color separating the Status Bar and editor when a program is being debugged.
    "statusBar.noFolderBackground": "--background1:background1_light--", // Status Bar background color when no folder is opened.
    "statusBar.noFolderForeground": "--white:white_light--", // Status Bar foreground color when no folder is opened.
    "statusBar.noFolderBorder": "--transparent:transparent_light--", // Status Bar border color separating the Status Bar and editor when no folder is opened.
    "statusBarItem.activeBackground": "--bluewhite20:bluewhite20_light--", // Status Bar item background color when clicking.
    "statusBarItem.hoverBackground": "--bluewhite20:bluewhite20_light--", // Status Bar item background color when hovering.
    "statusBarItem.prominentForeground": "--cyan:cyan_light--", // Status Bar prominent items foreground color.
    "statusBarItem.prominentBackground": "--bluewhite10:bluewhite10_light--", // Status Bar prominent items background color.
    "statusBarItem.prominentHoverBackground": "--bluewhite20:bluewhite20_light--", // Status Bar prominent items background color when hovering.
    "statusBarItem.remoteBackground": "--blue:hi_blue_light--", // Background color for the remote indicator on the status bar.
    "statusBarItem.remoteForeground": "--white:white_light--", // Foreground color for the remote indicator on the status bar.
    // Title bar
    "titleBar.activeBackground": "--background0:background0_light--", // Title Bar background when the window is active.
    "titleBar.activeForeground": "--bluewhite80:bluewhite80_light--", // Title Bar foreground when the window is active.
    "titleBar.inactiveBackground": "--background1:background1_light--", // Title Bar background when the window is inactive.
    "titleBar.inactiveForeground": "--bluewhite40:bluewhite40_light--", // Title Bar foreground when the window is inactive.
    "titleBar.border": "--background0:background0_light--", // Title bar border color.
    // Menu bar colors
    "menubar.selectionForeground": "--white:white_light--", // Foreground color of the selected menu item in the menubar.
    "menubar.selectionBackground": "--blue:blue_light--", // Background color of the selected menu item in the menubar.
    "menubar.selectionBorder": "--transparent:transparent_light--", // Border color of the selected menu item in the menubar.
    "menu.foreground": "--white:white_light--", // Foreground color of menu items.
    "menu.background": "--background2:background2_light--", // Background color of menu items.
    "menu.selectionForeground": "--white:white_light--", // Foreground color of the selected menu item in menus.
    "menu.selectionBackground": "--blue:hi_blue_light--", // Background color of the selected menu item in menus.
    "menu.selectionBorder": "--transparent:transparent_light--", // Border color of the selected menu item in menus.
    "menu.separatorBackground": "--transparent:transparent_light--", // Color of a separator menu item in menus.
    "menu.border": "--background2_border:background2_border_light--", // Border color of menus.
    // Integrated terminal colors
    "terminal.background": "--background0:background0_light--", // The background of the Integrated Terminal's viewport.
    "terminal.border": "--background1_border:background1_border_light--", // The color of the border that separates split panes within the terminal. This defaults to panel.border.
    "terminal.foreground": "--hi_white:hi_white_light--", // The default foreground color of the Integrated Terminal.
    "terminal.ansiBlack": "--black:black_light--", // 'Black' ANSI color in the terminal.
    "terminal.ansiBlue": "--blue:blue_light--", // 'Blue' ANSI color in the terminal.
    "terminal.ansiBrightBlack": "--hi_black:hi_black_light--", // 'BrightBlack' ANSI color in the terminal.
    "terminal.ansiBrightBlue": "--hi_blue:hi_blue_light--", // 'BrightBlue' ANSI color in the terminal.
    "terminal.ansiBrightCyan": "--hi_cyan:hi_cyan_light--", // 'BrightCyan' ANSI color in the terminal.
    "terminal.ansiBrightGreen": "--hi_green:hi_green_light--", // 'BrightGreen' ANSI color in the terminal.
    "terminal.ansiBrightMagenta": "--hi_magenta:hi_magenta_light--", // 'BrightMagenta' ANSI color in the terminal.
    "terminal.ansiBrightRed": "--hi_red:hi_red_light--", // 'BrightRed' ANSI color in the terminal.
    "terminal.ansiBrightWhite": "--hi_white:hi_white_light--", // 'BrightWhite' ANSI color in the terminal.
    "terminal.ansiBrightYellow": "--hi_yellow:hi_yellow_light--", // 'BrightYellow' ANSI color in the terminal.
    "terminal.ansiCyan": "--cyan:cyan_light--", // 'Cyan' ANSI color in the terminal.
    "terminal.ansiGreen": "--green:green_light--", // 'Green' ANSI color in the terminal.
    "terminal.ansiMagenta": "--magenta:magenta_light--", // 'Magenta' ANSI color in the terminal.
    "terminal.ansiRed": "--red:red_light--", // 'Red' ANSI color in the terminal.
    "terminal.ansiWhite": "--white:white_light--", // 'White' ANSI color in the terminal.
    "terminal.ansiYellow": "--yellow:yellow_light--", // 'Yellow' ANSI color in the terminal.
    "terminal.selectionBackground": "--white20:white20_light--", // The selection background color of the terminal.
    "terminalCursor.background": "--background0:background0_light--", // The background color of the terminal cursor. Allows customizing the color of a character overlapped by a block cursor.
    "terminalCursor.foreground": "--white:white_light--", // The foreground color of the terminal cursor.
    // Debug
    "debugToolBar.background": "--background2:background2_light--", // Debug toolbar background color.
    "debugToolBar.border": "--background2_border:background2_border_light--", // Debug toolbar border color.
    "editor.stackFrameHighlightBackground": "--blue40:blue40_light--", // Background color of the top stack frame highlight in the editor.
    "editor.focusedStackFrameHighlightBackground": "--blue:hi_blue_light--", // Background color of the focused stack frame highlight in the editor.
    "debugIcon.breakpointForeground": "--red:red_light--", // Icon color for breakpoints.
    "debugIcon.startForeground": "--magenta:magenta_light--", // Debug toolbar icon for start debugging.
    "debugIcon.pauseForeground": "--magenta:magenta_light--", // Debug toolbar icon for pause.
    "debugIcon.stopForeground": "--red:red_light--", // Debug toolbar icon for stop.
    "debugIcon.disconnectForeground": "--red:red_light--", // Debug toolbar icon for disconnect.
    "debugIcon.restartForeground": "--green:green_light--", // Debug toolbar icon for restart.
    "debugIcon.stepOverForeground": "--magenta:magenta_light--", // Debug toolbar icon for step over.
    "debugIcon.stepIntoForeground": "--magenta:magenta_light--", // Debug toolbar icon for step into.
    "debugIcon.stepOutForeground": "--magenta:magenta_light--", // Debug toolbar icon for step over.
    "debugIcon.continueForeground": "--magenta:magenta_light--", // Debug toolbar icon for continue.
    "debugIcon.stepBackForeground": "--magenta:magenta_light--", // Debug toolbar icon for step back.
    // Settings
    "settings.modifiedItemIndicator": "--blue:blue_light--" //The line that indicates a modified setting.
